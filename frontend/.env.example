# API base URL for backend integration
VITE_API_BASE_URL=http://localhost:8000/api/v1

# Mapbox access token for map rendering
VITE_MAPBOX_ACCESS_TOKEN=pk.your_mapbox_token_here

# Toggle mock API data when backend is not available
VITE_USE_MOCK_API=true
VITE_FALLBACK_TO_MOCK=false

# Control whether to load mock hotspots in DEV mode
VITE_DISABLE_MOCK_PREVIEW=false
